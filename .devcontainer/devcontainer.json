{
    "name": "eggforce smart contract",
    
    "dockerComposeFile": "docker-compose.yml",
    "service": "app",
	"workspaceFolder": "/workspace",

    "customizations": {
        "vscode": {
            "extensions": [
                "eamodio.gitlens",
                "rust-lang.rust-analyzer",
                "ms-vscode.cmake-tools"
            ]
        }
    },
    "postCreateCommand": "make prepare",
    "shutdownAction": "stopCompose",
    "remoteUser": "root"
}